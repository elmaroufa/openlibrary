<?xml version="1.0"?>

<odoo>

    <!-- Library app menu-->
    <menuitem id="menu_library" 
            name="Library" />
    <!-- Action to open Book -->
    <act_window id="action_library_book" name="Library Books"
    res_model="library.book" view_mode="tree, form" />

    <!-- Menu item to open book-->
    <menuitem id="menu_library_book"
        name="Books"
        parent="library_menu"
    action="action_library_book"/>
    
</odoo>